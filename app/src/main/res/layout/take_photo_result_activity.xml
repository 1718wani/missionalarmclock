<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/takePhotoResultNextBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="メニューへ"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="642dp" />

    <TextView
        android:id="@+id/takePhotoResultYell"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/take_photo_result_yell"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="505dp" />

    <TextView
        android:id="@+id/takePhotoResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/take_photo_result"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="306dp" />

</androidx.constraintlayout.widget.ConstraintLayout>